# EmailJS Configuration Template
# Copy this file to config.env and add your actual EmailJS keys

# Your EmailJS Public Key (starts with 'user_')
EMAILJS_PUBLIC_KEY=your_public_key_here

# Your EmailJS Service ID (starts with 'service_')  
EMAILJS_SERVICE_ID=your_service_id_here

# Your Email Template IDs
EMAILJS_APPOINTMENT_TEMPLATE=your_appointment_template_id
EMAILJS_CONTACT_TEMPLATE=your_contact_template_id

# EmailJS API URL
EMAILJS_API_URL=https://api.emailjs.com/api/v1.0/email/send
